{
   "name": "project",
   "version": "1.0.0",
   "main": "index.js",
   "type": "commonjs",
   "scripts": {
      "test": "cucumber-js --profile default",
      "test:ui": "cucumber-js --profile ui",
      "test:api": "cucumber-js --profile api",  
      "report": "npx open-cli reports/cucumber-report.html", 
      "report:ui": "npx open-cli reports/ui-cucumber-report.html", 
      "report:api": "npx open-cli reports/api-cucumber-report.html", 
      "report:ubuntu": "xdg-open reports/cucumber-report.html",
      "report:ui:ubuntu": "xdg-open reports/ui-cucumber-report.html",
      "report:api:ubuntu": "xdg-open reports/api-cucumber-report.html",
      "generate-ui-html-report": "cucumber-html-reporter --source Project/reports/ui-cucumber-report.json --output Project/reports/ui-cucumber-report.html",
      "generate-api-html-report": "cucumber-html-reporter --source Project/reports/api-cucumber-report.json --output Project/reports/api-cucumber-report.html"
   },
   "devDependencies": {
      "@babel/core": "^7.26.0",
      "@babel/preset-env": "^7.26.0",
      "@babel/register": "^7.25.9",
      "@cucumber/cucumber": "^11.1.1",
      "@cucumber/html-formatter": "^21.7.0",
      "@cucumber/pretty-formatter": "^1.0.0",
      "@playwright/test": "^1.49.1",
      "allure-commandline": "^2.32.0",
      "allure-cucumberjs": "^3.0.7",
      "allure-playwright": "^3.0.7",
      "axios": "^1.7.9",
      "chai": "^4.3.7",
      "cucumber": "^6.0.7",
      "open": "^10.1.0",
      "playwright": "^1.49.1",
      "rimraf": "^6.0.1",
      "cucumber-html-reporter": "^5.5.0"
   },
   "dependencies": {
      "@cucumber/cucumber": "^7.3.2",
      "allure-js-commons": "^3.0.7",
      "assert": "^2.0.0",
      "playwright": "^1.12.3"
   }
}
